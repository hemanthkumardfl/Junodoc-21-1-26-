<aura:component controller="JunoSignRemindersController"
                implements="flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickAction"
                access="global">
    
    <aura:attribute name="recordId" type="Id" />
    <aura:attribute name="ObjectAPIName" type="String" />
    <aura:attribute name="existingReminders" type="List" />

    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />

    <c:JunoSignReminders aura:id="reminderSection"
                         ObjectAPIName="{!v.ObjectAPIName}"
                         existingReminders="{!v.existingReminders}"
                         />
</aura:component>