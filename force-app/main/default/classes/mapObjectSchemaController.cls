    public void getObjectDatafromDestination(){
        if(objectAPIName == null || objectAPIName == ''){
            showError = true;
            ErrorMsg = 'Please Enter Object API Name';
        }
        else if(endPointName == 'None'){
            showError = true;
            ErrorMsg = 'Please Select Destination';
        }
        else{
            getDatafromDestination();  
            showError = false;
        } 
    }
