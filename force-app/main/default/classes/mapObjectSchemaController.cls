    public void saveMapSchema(){
        if(selectedSourceExternalID != null && selectedDestinationExternalID != null){
            JunoDoc__Junodoc_Object_Schema_Mapping__c mapSchemaInstance = new JunoDoc__Junodoc_Object_Schema_Mapping__c();
            
            
            if(Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.fields.Name.isUpdateable()
                && Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.fields.Name.isCreateable()){
                    mapSchemaInstance.Name = objectAPIName;
            }
            if(Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.fields.Destination_Name__c.isUpdateable()
                && Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.fields.Destination_Name__c.isCreateable()){
                    mapSchemaInstance.Destination_Name__c = endPointName;
            }
            if(Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.fields.Object_Name__c.isUpdateable()
                && Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.fields.Object_Name__c.isCreateable()){
                    mapSchemaInstance.Object_Name__c = objectAPIName;
            }
            if(Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.fields.Source_External_Id__c.isUpdateable()
                && Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.fields.Source_External_Id__c.isCreateable()){
                    mapSchemaInstance.Source_External_Id__c = selectedSourceExternalID;
            }
            if(Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.fields.Destination_External_Id__c.isUpdateable()
                && Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.fields.Destination_External_Id__c.isCreateable()){
                    mapSchemaInstance.Destination_External_Id__c = selectedDestinationExternalID;
            }
            String MissingFields = '';
            String MatchingFields = '';
            for(missingObjectSchemaInnerClass missingField:missingObjectSchemaRecordsLst){
                MissingFields = MissingFields + missingField.fieldName+',';
            }
            if(MissingFields != ''){
                MissingFields = MissingFields.removeEnd(',');  
            } 
            for(missingObjectSchemaInnerClass MatchingField:matchingObjectSchemaRecordsLst){
                MatchingFields = MatchingFields + MatchingField.fieldName+',';
            }
            if(MatchingFields !=''){
                MatchingFields = MatchingFields.removeEnd(',');        
            }  
            
            if(Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.fields.Matching_Fields__c.isUpdateable()
                && Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.fields.Matching_Fields__c.isCreateable()){
                    mapSchemaInstance.Matching_Fields__c = MatchingFields;
            }  
            if(Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.fields.Missing_Fields__c.isUpdateable()
                && Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.fields.Missing_Fields__c.isCreateable()){
                    mapSchemaInstance.Missing_Fields__c = MissingFields; 
            }  
            if(Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.fields.Schema_External_ID__c.isUpdateable()
                && Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.fields.Schema_External_ID__c.isCreateable()){
                    mapSchemaInstance.Schema_External_ID__c = objectAPIName + endPointName; 
            }
            if(Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.isUpdateable()
                && Schema.sObjectType.JunoDoc__Junodoc_Object_Schema_Mapping__c.isCreateable()){
                    upsert mapSchemaInstance Schema_External_ID__c;
            }
            
            List<JunoDoc__Junodoc_Object_RecordType_Mapping__c> mapRecordtypes = new List<JunoDoc__Junodoc_Object_RecordType_Mapping__c>();
            for(sourceDestinationRecordtypes sourceDestinationRecTypeRec : sourceDestinationRecordtypesinfo){
                if(sourceDestinationRecTypeRec.SourceRecordtypeName != 'Master'){
                    JunoDoc__Junodoc_Object_RecordType_Mapping__c recTypeMapping = new JunoDoc__Junodoc_Object_RecordType_Mapping__c(); 
                   // if (Schema.sObjectType.JunoDoc__Junodoc_Object_RecordType_Mapping__c.fields.Object__c.isAccessible()) {
                    //JunoDoc__Junodoc_Object_RecordType_Mapping__c.Object__c.getDescribe().isAccessible();
              if (Schema.sObjectType.JunoDoc__Junodoc_Object_RecordType_Mapping__c.fields.Object__c.isCreateable()
               && Schema.sObjectType.JunoDoc__Junodoc_Object_RecordType_Mapping__c.fields.Object__c.isUpdateable()) {
                    recTypeMapping.Object__c = mapSchemaInstance.id;
                    }
                    
                    if(Schema.sObjectType.JunoDoc__Junodoc_Object_RecordType_Mapping__c.fields.Source_Record_Type__c.isCreateable()
                        && Schema.sObjectType.JunoDoc__Junodoc_Object_RecordType_Mapping__c.fields.Source_Record_Type__c.isUpdateable()){
                            recTypeMapping.Source_Record_Type__c = sourceDestinationRecTypeRec.SourceRecordtypeName;
                    }
                    if(Schema.sObjectType.JunoDoc__Junodoc_Object_RecordType_Mapping__c.fields.Destination_Record_Type__c.isCreateable()
                        && Schema.sObjectType.JunoDoc__Junodoc_Object_RecordType_Mapping__c.fields.Destination_Record_Type__c.isUpdateable()){
                            recTypeMapping.Destination_Record_Type__c = mapDestinationRecordtypeID.get(sourceDestinationRecTypeRec.DestinationRecordtypeID);
                    }
                    if(Schema.sObjectType.JunoDoc__Junodoc_Object_RecordType_Mapping__c.fields.Record_Type_Map_External_ID__c.isCreateable()
                        && Schema.sObjectType.JunoDoc__Junodoc_Object_RecordType_Mapping__c.fields.Record_Type_Map_External_ID__c.isUpdateable()){
                            recTypeMapping.Record_Type_Map_External_ID__c = endPointName + sourceDestinationRecTypeRec.SourceRecordtypeID;
                    }
                    if(Schema.sObjectType.JunoDoc__Junodoc_Object_RecordType_Mapping__c.fields.Destination_Record_Type_Id__c.isCreateable()
                        && Schema.sObjectType.JunoDoc__Junodoc_Object_RecordType_Mapping__c.fields.Destination_Record_Type_Id__c.isUpdateable()){
                            recTypeMapping.Destination_Record_Type_Id__c = sourceDestinationRecTypeRec.DestinationRecordtypeID;
                    }
                    if(Schema.sObjectType.JunoDoc__Junodoc_Object_RecordType_Mapping__c.fields.Source_Record_Type_Id__c.isCreateable()
                       && Schema.sObjectType.JunoDoc__Junodoc_Object_RecordType_Mapping__c.fields.Source_Record_Type_Id__c.isUpdateable()){
                    recTypeMapping.Source_Record_Type_Id__c = sourceDestinationRecTypeRec.SourceRecordtypeID;
                    }
                    mapRecordtypes.add(recTypeMapping);
                }
            }
            
            if(Schema.sObjectType.JunoDoc__Junodoc_Object_RecordType_Mapping__c.isCreateable()
               && Schema.sObjectType.JunoDoc__Junodoc_Object_RecordType_Mapping__c.isUpdateable()){
                    upsert  mapSchemaInstance;
            }
            
        }
        else{
            //displayExternalIdPopUp = true;
            showError = true;
            ErrorMsg = 'ExternalId field is Required in Source & Destinaion. If you already created the External ids, Please Compare the Schema Again.';
